{
  "name": "abms",
  "homepage": "https://allbritishmotorshow.com",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18",
    "npm": ">=6"
  },
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build && tsc -p tsconfig.sitemap.json && node src/scripts/generate-sitemap.js",
    "serve": "vite preview",
    "deploy": "gh-pages -d dist",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'"
  },
  "dependencies": {
    "@babel/eslint-parser": "7.17.0",
    "@steveeeie/react-page-transition": "1.2.1",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "12.6.2",
    "@vitejs/plugin-react": "4.5.2",
    "bootstrap": "5.1.3",
    "chalk": "4.1.0",
    "framer-motion": "12.16.0",
    "google-map-react": "2.2.5",
    "html-react-parser": "5.2.5",
    "list-react-files": "0.2.0",
    "path": "0.12.7",
    "prop-types": "15.8.1",
    "react": "18.3.1",
    "react-bootstrap": "2.10.10",
    "react-dom": "18.3.1",
    "react-error-overlay": "6.0.10",
    "react-icons": "5.5.0",
    "react-lazy-load-image-component": "1.6.3",
    "react-responsive-carousel": "3.2.23",
    "react-router": "7.6.2",
    "react-router-dom": "7.6.2",
    "react-transition-group": "4.4.5",
    "rollup-plugin-visualizer": "6.0.3",
    "sass": "1.49.9",
    "styled-components": "5.3.3",
    "vite-plugin-svgr": "4.3.0"
  },
  "devDependencies": {
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7",
    "eslint": "8.17.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "gh-pages": "6.3.0",
    "json-loader": "0.5.7",
    "lint-staged": "10.5.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "sitemap": "8.0.0",
    "vite": "6.3.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "lint": "eslint .",
  "lint:fix": "eslint --fix .",
  "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
}
